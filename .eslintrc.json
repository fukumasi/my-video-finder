{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-unused-vars": "warn",
    "comma-dangle": ["error", "never"],
    "no-undef": "off"
  }
}
